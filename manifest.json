{
    "manifest_version": 3,
    "name": "Tracker Watch",
    "version": "1.0",
    "description": "Opozarja na third-party trackerje na spletnih straneh.",
    "permissions": ["storage", "tabs", "webRequest"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }